spring:
  redis:
    host: localhost
    port: 6379
    database: 0
    password: # 如果需要密码，请填写

io:
  github:
    surezzzzzz:
      sdk:
        auth:
          aksk:
            client:
              enable: true
              client-id: YOUR_CLIENT_ID  # 替换为真实的 Client ID
              client-secret: YOUR_CLIENT_SECRET  # 替换为真实的 Client Secret
              server-url: http://localhost:8080  # 替换为真实的服务器地址
              token-endpoint: /oauth2/token
              token:
                refresh-before-expire: 300
              redis:
                token:
                  me: feign-test-app

# Feign 配置
feign:
  client:
    config:
      default:
        connectTimeout: 5000
        readTimeout: 30000

# 测试服务器配置
test:
  server:
    url: http://localhost:8080  # 替换为真实的测试服务器地址

# 日志配置
logging:
  level:
    io.github.surezzzzzz.sdk.auth.aksk: DEBUG
    feign: DEBUG
